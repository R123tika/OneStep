<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="../css/bootstrap.min.css">
    <link rel="stylesheet" href="css/index.css">
    <title>Document</title>
</head>
<body>
    <script src="../jq/jquery.js" type="text/javascript"></script>
    <script src="../js/bootstrap.js" type="text/javascript"></script>
    {{>headers}}

    <div class="container-fluid mt-5 about-style" id="serviceid">
        <img src="./deer.jpg" alt="">
        <div class="row">
            <div class="col-md-10 col-12 mx-auto">
                <h1 class="text-center main-heading">Our Services</h1>
                <br>
                <p class="text-center sub-heading"></p>
                <div class="row">
                   {{#each data}}
                    <div class="col-md-4">
                        <div class="box">
                            <div class="our-services {{this.color}}">
                                <div class="icon">
                                <img src="image/fil.png" alt="loading.." style="width: 20%;">
                                </div>
                                <h4 style ="color: #fd5c63 ">{{this.Name}}</h4>
                                <br>
                                <p>NGO-{{this.NGO}}</p>
                                <h4>establish in {{this.Establish}}</h4>
                                <h3>Current Member-{{this.Member}}</h3>
                                <p>{{this.Detail}}</p>
                            </div>
                        </div>
                    </div>
                    {{/each}}
                    
                </div>
            </div>
        </div>
    </div>             
</body>
</html>
{{>map}}
 {{>footer}} 